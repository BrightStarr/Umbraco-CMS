{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Umbraco.Core/Umbraco.Core.csproj",
            "Umbraco.Web/Umbraco.Web.csproj"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "_site/**"
          ],
          "cwd": "../src/"
        }
      ],
      "dest": "../apidocs/csharp",
      "filter": "docfx.filter.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "csharp/**.yml",
          "csharp/index.md"
        ],
        "cwd": "../apidocs"
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../apidocs/_site",
    "template": [
      "default"
    ]
  }
}